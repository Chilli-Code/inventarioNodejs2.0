<style>
  .info-productos div span {
    display: flex;
    gap: 5px;
  }

  .info-productos span {
    margin: 0;
    font-weight: 500;
    color: var(--app-content-main-color);
  }

  .info-productos {
    justify-content: space-between;
    display: flex;
    padding: 0px 5px;
    margin-top: auto;
  }

  .info-productos div {
    justify-content: space-between;
    display: flex;
    width: 100%;
    flex-wrap: wrap;
  }

  .bg-rls {
    padding: 5px;
    border-radius: 10px;
  }

  .bg-rls:nth-child(1) {
    background-color: rgb(40 105 255 / 16%);
  }

  .bg-rls:nth-child(2) {
    background-color: rgba(43, 169, 114, 0.2);

  }

  .bg-rls:nth-child(3) {
    background-color: rgba(89, 113, 157, 0.2);
  }
</style>
